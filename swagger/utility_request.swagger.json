  "paths": {
    "/survey/api/v1/utility_requests": {
      "get": {
        "summary": "Maps the ListUtilityRequest method to a GET request on /survey/v1/utility_request",
        "operationId": "UtilityServices_ListUtilityServices",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/utilityListUtilityResponse"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "page",
            "in": "query",
            "required": false,
            "type": "integer",
            "format": "int32"
          },
          {
            "name": "page_size",
            "in": "query",
            "required": false,
            "type": "integer",
            "format": "int32"
          },
          {
            "name": "utility_number",
            "in": "query",
            "required": false,
            "type": "string"
          }
        ],
        "tags": [
          "UtilityServices"
        ]
      }
    },
    "/survey/api/v1/utility_requests/{utility_number}": {
      "get": {
        "operationId": "UtilityServices_FetchUtilityRequest",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/utilityUtilityServiceResponse"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "utility_number",
            "description": "Get the Utility Number",
            "in": "path",
            "required": true,
            "type": "string"
          }
        ],
        "tags": [
          "UtilityServices"
        ]
      }
    }
  },
